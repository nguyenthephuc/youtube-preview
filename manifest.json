{
    "author": "The Phuc",
    "version": "1.0",
    "name": "Youtube preview",
    "short_name": "Youtube preview",
    "manifest_version": 2,
    "content_security_policy": "script-src 'self'; object-src 'self'",
    "description": "Fast preview video on youtube",
    "browser_action": {
       "default_icon": "images/icon.png"
    },
    "permissions": [
        "storage",
        "tabs",
        "activeTab",
        "https://*.youtube.com/"
    ],
    "icons": {
        "16": "images/icon_16.png",
        "48": "images/icon_48.png",
        "128": "images/icon.png"
    },
    "background": {
        "scripts": [
            "js/background.js"
        ]
    },
    "web_accessible_resources": []
}